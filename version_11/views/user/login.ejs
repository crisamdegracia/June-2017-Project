<% include ../partials/header %>

<section>
    <div class="container mt-5 pt-5">
        <div class="col-xs-12 col-md-6 col-sm-8 col-lg-6 m-auto">
            <h2>Log in </h2>
            <form action="/login" method="post">
               
                <input name="username" type="text" class="form-control mb-2" placeholder="Enter Username" autofocus required>
                <input name="password" type="password" class="form-control mb-2" placeholder="Enter Password" required>
                <button class="btn btn-primary mb-5">Submit</button>
            </form>
        </div>
    </div>
</section>

