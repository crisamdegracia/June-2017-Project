<% include  ../partials/header %>

<section id="registerMainHead" class="my-5 py-3">
    <div class="container">
       <h1 class="text-center">Sign up!</h1>
        <form action="/register" method="post">
           <div class="form-group col-md-6 col-sm-6 m-auto">
            <input type="text" name="username" autofocus required class="form-control" placeholder="Your username here">
            </div>
             <div class="form-group col-md-6 col-sm-6 m-auto">
            <input type="password" placeholder="Your password here" name="password" required class="form-control my-2 py-2">
            </div>
            <div class="text-center my-2">
            <button class="btn btn-primary">Register</button>
            </div>
        </form>
    </div>
</section>
